t firebase from 'firebase'\r\n    export default{\r\n        data(){\r\n            return{\r\n                link:'',\r\n                url:'',\r\n\r\n            }\r\n        },\r\n        name:'editFooter',\r\n        props:{\r\n            linkData:Object,\r\n        },\r\n        methods:{\r\n            createLink(){\r\n                firebase.database().ref('link').push({\r\n                    link:this.link,\r\n                    URL:this.url,\r\n                })\r\n            },\r\n            removeLink(index){\r\n                firebase.database().ref(`link/${index}`).remove()\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n</script>"],"sourceRoot":"src/views"}]}