          <mdb-card-text>\r\n              <p style=\"white-space: pre-wrap;\">{{ data.content }}</p>\r\n            </mdb-card-text>\r\n\r\n            \r\n          </mdb-card-body>\r\n\r\n\r\n        </mdb-card>\r\n\r\n\r\n          <!-- <mdb-list-group v-for=\"(data,index) in reverseItems\" :key=\"index\">\r\n            <mdb-list-group-item v-if=\"data.category === 'study'\" icon=\"pen\"> <router-link to=\"study\">{{data.content}}</router-link></mdb-list-group-item>\r\n            <mdb-list-group-item v-if=\"data.category === 'achievement'\" icon=\"book\"> <router-link to=\"achievement\">{{data.content}}</router-link></mdb-list-group-item>\r\n            <mdb-list-group-item v-if=\"data.category === 'member'\" icon=\"users\"> <router-link to=\"member\"></router-link>{{data.content}}</mdb-list-group-item>\r\n            <mdb-list-group-item v-if=\"data.category === 'acsess'\" icon=\"map-marker-alt\"> <router-link to=\"acsess\">{{data.content}}</router-link></mdb-list-group-item>\r\n\r\n          </mdb-list-group> -->\r\n\r\n\r\n        <!-- <button @click=\"moreView\">MORE</button> -->\r\n        <div class=\"d-flex flex-row-reverse\">\r\n            <mdb-btn class=\"z-depth-0 mr-4\" @click=\"moreView\" color=\"success\">MORE</mdb-btn>\r\n        </div>\r\n\r\n  \r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mdbBtn,\r\n        mdbCarousel, \r\n        mdbCard,\r\n        mdbCardBody,\r\n        mdbCardTitle,\r\n        mdbCardText,\r\n        mdbIcon  } from 'mdbvue';\r\n\r\n  export default{\r\n    name: 'home',\r\n    data(){\r\n      return{\r\n        homeCategory:'study',\r\n        homeContent:'',\r\n        carouselData:\"\"\r\n      }\r\n    },\r\n    props:{\r\n      newsData:Object,\r\n      homeData:String,\r\n      eyeCatchData:Object,\r\n    },\r\n    components: {\r\n      mdbBtn, mdbCarousel,\r\n      mdbCard,\r\n        mdbCardBody,\r\n        mdbCardTitle,\r\n        mdbCardText,\r\n        mdbIcon\r\n    },\r\n    watch: {\r\n      // この関数は question が変わるごとに実行されます。\r\n      eyeCatchData: function () {\r\n        const eyeCatchData = this.eyeCatchData\r\n      this.carouselData=Object.keys(eyeCatchData).map(function (key) {return eyeCatchData[key]})\r\n      }\r\n    },\r\n    computed:{\r\n      reverseItems() {\r\n        const datas = this.newsData;\r\n        const sorted = {};\r\n        var array = [];\r\n        for (let key in datas) {\r\n        //指定された名前のプロパティがオブジェクトにあるかどうかチェック\r\n          if (datas.hasOwnProperty(key)) {\r\n            //if条件がtrueならば，配列の最後にキーを追加する\r\n            array.push(key);\r\n        }}\r\n        array.reverse();\r\n        for (var i = 0; i < array.length; i++) {\r\n        /*戻り値用のオブジェクトに\r\n        新オブジェクト[配列内のキー] ＝ 引数のオブジェクト[配列内のキー]を入れる．\r\n        配列はソート済みなので，ソートされたオブジェクトが出来上がる*/\r\n        sorted[array[i]] = datas[array[i]];\r\n    }\r\n    //戻り値にソート済みのオブジェクトを指定\r\n    return sorted;\r\n\r\n    }\r\n    },\r\n    methods:{\r\n      moreView(){\r\n        this.$emit('moreView');\r\n      },\r\n      goStudy(){\r\n            this.$router.push('/study')\r\n        },\r\n    },\r\n    mounted(){\r\n      const eyeCatchData = this.eyeCatchData\r\n      this.carouselData=Object.keys(eyeCatchData).map(function (key) {return eyeCatchData[key]})\r\n      console.log(\"カルーセル\")\r\n\r\n      console.log(this.carouselData)\r\n    },\r\n    \r\n\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.homeImgBox{\r\n  position: relative;\r\n  background-color:black;\r\n}\r\n.homeImgBox::before{\r\n  content: \"\";\r\n    display: block;\r\n    padding-top: 105%;\r\n}\r\n\r\n.carouselImg {\r\n  position: absolute;\r\n    top: 0; \r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.homeImgBox .home-btn {\r\n  position: absolute;/*絶対配置*/\r\n  bottom: 0;\r\n  right: 0;\r\n  }\r\n\r\n\r\n</style>"]}]}